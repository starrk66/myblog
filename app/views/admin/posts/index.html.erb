
  <table class="table table-hover datatable">
  <thead>
        <tr>
          <th></th>
          <th>title</th>
          <th>author</th>
          <th>published_at</th>
          <th></th>
        </tr>
      </thead>
  <% @posts.each  do|p| %>


    <tr>
      <td><%= p.id %></td>
      <td><%= p.title %></td>
      <td><%= p.author %></td>
      <td><%= l p.published_at.getlocal %></td>
      <td>
        <%= link_to "edit", edit_admin_post_path(p) %>
        <%= link_to 'delete', admin_post_path(p), :method => :delete, :data => {:confirm => "are you sure"} %>
      </td>
    </tr>
    <% end %>
  </table>

